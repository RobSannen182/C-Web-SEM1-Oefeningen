@model IEnumerable<string>

<div class="btn btn-group-vertical w-100" role="group">
    <a class="btn btn-outline-info my-1" asp-action="Index">Overzicht</a>

    @foreach (string functie in Model)
    {
        <a class="btn btn-outline-info my-1 @(functie == ViewBag.SelectedCategory ? "btn-primary" : "btn-outline-info")"
           asp-action="Index"
           asp-controller="Gebruikers"
           asp-route-functie="@functie">
            @functie
        </a>
    }
</div>